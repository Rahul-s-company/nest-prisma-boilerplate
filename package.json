{
  "name": "NestjsBoilerpate",
  "private": true,
  "version": "1.0.0",
  "description": "Nest.js Node app",
  "author": "rahul.b",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "npx prisma migrate deploy && npm run prisma:seed && nest start --watch",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:seed": "ts-node prisma/seeds/index.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-chime": "^3.629.0",
    "@aws-sdk/client-chime-sdk-identity": "^3.629.0",
    "@aws-sdk/client-chime-sdk-messaging": "^3.629.0",
    "@aws-sdk/client-s3": "^3.626.0",
    "@aws-sdk/client-ses": "^3.624.0",
    "@aws-sdk/client-sqs": "^3.454.0",
    "@aws-sdk/credential-providers": "^3.629.0",
    "@aws-sdk/s3-request-presigner": "^3.626.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.1.1",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.4.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/websockets": "^10.4.1",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.0",
    "@prisma/client": "^4.4.0",
    "aws-sdk": "^2.1671.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv": "^6.3.9",
    "date-fns": "^4.1.0",
    "docx": "^8.5.0",
    "dotenv": "^8.2.0",
    "exceljs": "^4.4.0",
    "ics": "^3.7.6",
    "ioredis": "^5.4.1",
    "jsforce": "^3.2.0",
    "mailcomposer": "^4.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "pptxgenjs": "^3.12.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.1.6",
    "webpack": "^5.91.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/testing": "^10.3.9",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.14.6",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/schematics": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express-rate-limit": "^6.7.0",
    "husky": "^6.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.2.1",
    "prisma": "^4.4.0",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seeds/index.ts"
  }
}